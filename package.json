{
  "name": "breederhq",
  "version": "0.1.0",
  "private": true,
  "packageManager": "npm@9.9.3",
  "engines": {
    "node": ">=22 <23"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:contacts": "npm run dev -w @bhq/contacts",
    "dev:animals": "npm run dev -w @bhq/animals",
    "dev:breeding": "npm run dev -w @bhq/breeding",
    "dev:offspring": "npm run dev -w @bhq/offspring",
    "dev:all": "concurrently \"npm run dev:contacts\" \"npm run dev:animals\" \"npm run dev:breeding\" \"npm run dev:offspring\"",
    "dev": "npm run dev:all",
    "build": "echo \"root build noop\"",
    "lint": "eslint . --ext .ts,.tsx --max-warnings=0",
    "format": "prettier --write \"**/*.{ts,tsx,js,json,css,md}\"",
    "postinstall": "echo \"root postinstall: no builds here\""
  },
  "dependencies": {
    "clsx": "^2.1.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^24.3.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.34.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "prettier": "^3.3.3",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.5.4"
  }
}
